<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_hr_shift_change_order" model="ir.sequence">
            <field name="name">Shift Change Request Sequence</field>
            <field name="code">hr.shift.change.order</field>
            <field name="prefix">SCR-%(range_year)s%(month)s-</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">5</field>
            <field name="use_date_range">True</field>
        </record>
    </data>
</odoo>